{
  "name": "@vibe-creator/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "10.0.2",
    "@fastify/helmet": "13.0.1",
    "@fastify/multipart": "9.0.3",
    "@fastify/rate-limit": "10.2.2",
    "@prisma/client": "6.1.0",
    "@vibe-creator/shared": "workspace:*",
    "argon2": "0.41.1",
    "bullmq": "5.34.3",
    "dotenv": "16.4.7",
    "fastify": "5.3.2",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "5.4.2",
    "nanoid": "5.0.9",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "22.10.2",
    "@vitest/coverage-v8": "^4.0.16",
    "prisma": "6.1.0",
    "tsc-alias": "^1.8.10",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "4.0.16"
  }
}
